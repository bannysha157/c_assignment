#include<stdio.h>
int main()
{
//char//
	unsigned char a=128;//0 to 255
	char b=129;//-128 to 127
	printf("%d...%d\n",a,b);
  //int//
	int c=-23;
	unsigned int d=-21;
	printf("%d...%u\n",c,d);
	unsigned short int k=65538; //0 to 65535
	short int m=-35;
	printf("%u...%d\n",k,m);
//float & double//	
	float e=8.0;
	double f=7.5;
	printf("%e...%f",e,f);
}
//output=128.......-127
-23.....4294967275
2....-35
8.000000......7.500000
